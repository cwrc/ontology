  owl:subClass <http://www.cwrc.ca/ontologies/cwrc#CulturalFormation> ;
  skos:definition "Poor, destitute, unemployed, on social security."@en, ""@fr, ""@en .

<http://www.cwrc.ca/ontologies/cwrc#shopkeepers>
  a <http://www.cwrc.ca/ontologies/cwrc#SocialClassIdentity>, skos:Concept ;
  rdfs:label "shopkeepers"@en, "détaillant"@fr ;
