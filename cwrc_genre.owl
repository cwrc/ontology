  <cwrc:Genre rdf:about="#genreScholarship">
    <rdf:value>SCHOLARSHIP</rdf:value>
    <rdfs:label xml:lang="en">scholarship</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
