  <cwrc:Genre rdf:about="#genreHagiography">
    <rdf:value>HAGIOGRAPHY</rdf:value>
    <rdfs:label xml:lang="en">hagiography</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
