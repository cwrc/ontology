  <cwrc:Genre rdf:about="#genreEpithalamium">
    <rdf:value>EPITHALAMIUM</rdf:value>
    <rdfs:label xml:lang="en">epithalamium</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
