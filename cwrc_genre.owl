  <cwrc:Genre rdf:about="#genreCookbook">
    <rdf:value>COOKBOOK</rdf:value>
    <rdfs:label xml:lang="en">cookbook</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
