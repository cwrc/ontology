<cwrc:SocialClassIdentity rdf:about="#skilledCraftpersonArtisan">
 <rdfs:label xml:lang="en">This category includes such trades as goldsmith, tailor, shoemaker, milliner, and dressmaker.</rdfs:label>
 <rdfs:label xml:lang="fr">Artisan</rdfs:label>
 <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
