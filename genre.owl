  <genre:Genre rdf:about="#genreCharacter">
    <rdf:value>CHARACTER</rdf:value>
    <rdfs:label xml:lang="en">character</rdfs:label>
    <rdfs:label xml:lang="fr">personnage</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
