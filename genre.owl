  <genre:Genre rdf:about="#genreManifesto">
    <rdf:value>MANIFESTO</rdf:value>
    <rdfs:label xml:lang="en">manifesto</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
