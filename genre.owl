  <genre:Genre rdf:about="#genreCaptivitynarrative">
    <rdf:value>CAPTIVITYNARRATIVE</rdf:value>
    <rdfs:label xml:lang="en">captivity narrative</rdfs:label>
    <rdfs:label xml:lang="fr">récit de captivité</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
