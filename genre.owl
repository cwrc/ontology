    <rdf:value>APHORISM</rdf:value>
    <rdfs:label xml:lang="en">aphorism</rdfs:label>
    <rdfs:label xml:lang="fr">aphorisme</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
