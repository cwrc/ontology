  <genre:Genre rdf:about="#genreDidactic">
    <rdf:value>DIDACTIC</rdf:value>
    <rdfs:label xml:lang="en">didactic</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
