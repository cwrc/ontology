  <rdf:Description rdf:about="#genreFolksong">
    <rdf:value>FOLKSONG</rdf:value>
    <rdfs:label xml:lang="en">folk song</rdfs:label>
    <rdfs:label xml:lang="fr">chanson populaire</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
