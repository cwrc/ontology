  <rdf:Description rdf:about="#genreMoralitymysteryplay">
    <rdf:value>MORALITYMYSTERYPLAY</rdf:value>
    <rdfs:label xml:lang="en">morality or mystery play</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
