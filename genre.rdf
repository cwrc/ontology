    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">"Visual works created using multiple media." (<![CDATA[<a href="#cwrc:db2f8571-a773-4068-a35f-34262950bb8f">Getty, 2017</a>]]>)</skos:definition>
    <prov:wasDerivedFrom rdf:resource="http://vocab.getty.edu/aat/300404586"/>
