  <rdf:Description rdf:about="#genreQuiz">
    <rdf:value>QUIZ</rdf:value>
    <rdfs:label xml:lang="en">quiz</rdfs:label>
    <rdfs:label xml:lang="fr">quiz</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
