  </genre:LiteraryGenre>
  <genre:LiteraryGenre rdf:about="#elegy">
    <rdf:value>ELEGY</rdf:value>
    <rdfs:label xml:lang="en">elegy</rdfs:label>
    <rdfs:label xml:lang="fr">élégie</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
