  <rdf:Description rdf:about="#genreBurletta">
    <rdf:value>BURLETTA</rdf:value>
    <rdfs:label xml:lang="en">burletta</rdfs:label>
    <rdfs:label xml:lang="fr">burletta</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
