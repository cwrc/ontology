  <rdf:Description rdf:about="#genrePhilosophy">
    <rdf:value>PHILOSOPHY</rdf:value>
    <rdfs:label xml:lang="en">philosophy</rdfs:label>
    <rdfs:label xml:lang="fr">philosophie</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
