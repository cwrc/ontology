  <rdf:Description rdf:about="#genreComputerprogram">
    <rdf:value>COMPUTERPROGRAM</rdf:value>
    <rdfs:label xml:lang="en">computer program</rdfs:label>
    <rdfs:label xml:lang="fr">programme informatique</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
