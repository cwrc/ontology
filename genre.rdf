  <rdf:Description rdf:about="#genreShortstory">
    <rdf:value>SHORTSTORY</rdf:value>
    <rdfs:label xml:lang="en">short story</rdfs:label>
    <rdfs:label xml:lang="fr">nouvelle</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
