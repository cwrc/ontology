  <rdf:Description rdf:about="#genreDialogueofthedead">
    <rdf:value>DIALOGUEOFTHEDEAD</rdf:value>
    <rdfs:label xml:lang="en">dialogue of the dead</rdfs:label>
    <rdfs:label xml:lang="fr">dialogue des morts</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
