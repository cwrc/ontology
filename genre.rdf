  </rdf:Description>
  <rdf:Description rdf:about="#genreTextbook">
    <rdf:value>TEXTBOOK</rdf:value>
    <rdfs:label xml:lang="en">textbook</rdfs:label>
    <rdfs:label xml:lang="fr">manuel scolaire</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
