  <rdf:Description rdf:about="#genreJuvenilia">
    <rdf:value>JUVENILIA</rdf:value>
    <rdfs:label xml:lang="en">juvenilia</rdfs:label>
    <rdfs:label xml:lang="fr">oeuvres de jeunesse</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
