  <rdf:Description rdf:about="#genreDreamvision">
    <rdf:value>DREAMVISION</rdf:value>
    <rdfs:label xml:lang="en">dream vision</rdfs:label>
    <rdfs:label xml:lang="fr">vision onirique</rdfs:label>
