    <rdfs:comment xml:lang="fr">Désapprouver en faveur de l'instance @@#literaryCriticism@@.</rdfs:comment>
    <dcterms:isReplacedBy rdf:resource="#literaryCriticism"/>
    <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    <vs:term_status xml:lang="en">deprecated</vs:term_status>
