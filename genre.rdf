  <rdf:Description rdf:about="#genreTheology">
    <rdf:value>THEOLOGY</rdf:value>
    <rdfs:label xml:lang="en">theology</rdfs:label>
    <rdfs:label xml:lang="fr">théologie</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
