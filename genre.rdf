  <rdf:Description rdf:about="#genreEncyclopaedia">
    <rdf:value>ENCYCLOPAEDIA</rdf:value>
    <rdfs:label xml:lang="en">encyclopaedia</rdfs:label>
    <rdfs:label xml:lang="fr">encyclopédie</rdfs:label>
