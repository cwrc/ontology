  </genre:DigitalMedium>
  <genre:HistoricalGenre rdf:about="#historicalOverview">
    <skos:definition xml:lang="en">"Written works or other forms of media that describe or summarize a specific historical period or set of events, and their significance."</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <rdfs:label xml:lang="en">historical overview</rdfs:label>
