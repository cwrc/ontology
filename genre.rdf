    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <rdfs:label xml:lang="en">catalogue</rdfs:label>
  </genre:CatalogueForm>
  <genre:ThematicGenre rdf:about="#architecturalWriting">
    <skos:altLabel xml:lang="en">architecture</skos:altLabel>
    <skos:definition xml:lang="en">Written works concerning the art or science of designing and building structures, especially habitable structures, in accordance with principles determined by aesthetic and practical or material considerations. <![CDATA[<a href="#cwrc:bd595bc4-5991-4c51-98d9-9ab52c416f4f">(Getty, 2021)</a>]]></skos:definition>
    <rdfs:comment xml:lang="en">The description for this term is indebted to the Getty Art &amp; Architecture Thesaurus.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <rdfs:label xml:lang="en">architectural writing</rdfs:label>
    <rdf:type rdf:resource="#InformationalGenre"/>
    <rdf:type rdf:resource="#ScholarlyGenre"/>
  </genre:ThematicGenre>
  <genre:PoeticGenre rdf:about="#anacreontic">
    <skos:definition xml:lang="en">"Anacreontics are verses in a meter used by the Greek poet Anacreon in his poems dealing with love and wine. His later Greek imitators (whose surviving poems are known as the Anacreontea) took up the same themes and used the Anacreontic meter. In modern poetry, Anacreontics are short lyrical pieces that keep the Anacreontic subject matter but not the meter." <![CDATA[<a href="#cwrc:727d97bc-af78-4122-94be-aa78b2e97d5a">(DBpedia, 2019)</a>]]></skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <rdfs:label xml:lang="en">anacreontic</rdfs:label>
  </genre:PoeticGenre>
  <genre:ScholarlyGenre rdf:about="#maThesis">
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <rdfs:label xml:lang="en">M.A. thesis</rdfs:label>
  </genre:ScholarlyGenre>
  <genre:PoeticGenre rdf:about="#prosePoem">
    <skos:definition xml:lang="en">"Prose poetry is poetry written in prose instead of using verse but preserving poetic qualities such as heightened imagery, parataxis and emotional effects." <![CDATA[<a href="#cwrc:727d97bc-af78-4122-94be-aa78b2e97d5a">(DBpedia, 2019)</a>]]></skos:definition>
    <owl:sameAs rdf:resource="http://id.loc.gov/authorities/genreForms/gf2014026488"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/genre-ONTOLOGY_DATE"/>
    <rdfs:label xml:lang="en">prose poem</rdfs:label>
  </genre:PoeticGenre>
  <genre:CartographicForm rdf:about="#atlas">
