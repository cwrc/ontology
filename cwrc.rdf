  <cwrc:PoliticalAffiliation rdf:about="#housingMovement">
    <skos:inScheme rdf:resource="#PoliticalAffiliation"/>
    <rdf:value>housing activism</rdf:value>
    <rdf:value>housing charity</rdf:value>
    <rdfs:label xml:lang="en">housing movement</rdfs:label>
