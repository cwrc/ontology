    <owl:equivalentClass rdf:resource="#CulturalForm"/>
    <dcterms:isReplacedBy rdf:resource="#CulturalForm"/>
    <vs:term_status xml:lang="en">deprecated</vs:term_status>
  </owl:DeprecatedClass>

