  <!-- Change Sets -->

  <owl:Class rdf:ID="ChangeSet">
    <rdfs:label xml:lang="en">change set</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">A Changeset represents changes or additions made to the ontology by ontology editors or through users where instances were affected. Change Sets will also be used to translate and contain Orlando recordInfo instances.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2006/time/Instant"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
    <rdfs:range rdf:resource="skos:note"/>
    <prov:wasDerivedFrom rdf:resource="http://vocab.org/changeset/"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#affectedEntity">
    <rdfs:label xml:lang="en">affected entity</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">Affected Entities link to the Object or Class that was modified within a ChangeSet, this can be any rdf:resource. A @@#ChangeSet@@ may have 0 to many AffectedEntities.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:domain rdf:resource="#ChangeSet"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#alteredBy">
