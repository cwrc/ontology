    <skos:definition xml:lang="fr"/>
    <rdf:type rdf:resource="#Award"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="LiteraryAward">
    <rdfs:label xml:lang="en">literary award</rdfs:label>
    <rdfs:label xml:lang="fr"></rdfs:label>
    <skos:definition xml:lang="en">A literary award is an award presented in recognition of a particularly lauded literary piece or body of work. It is normally presented to an author.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdf:type rdf:resource="#Award"/>
    <owl:sameAs rdf:resource="http://dbpedia.org/resource/Literary_award"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

