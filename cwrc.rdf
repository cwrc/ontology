  <cwrc:PoliticalAffiliation rdf:about="#anti-War">
    <skos:inScheme rdf:resource="#PoliticalAffiliation"/>
    <rdf:value>anti-war</rdf:value>
    <rdf:value>anti-war activism</rdf:value>
    <rdf:value>vietnam war protester</rdf:value>
