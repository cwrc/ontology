    <rdfs:label xml:lang="fr">Juif</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="#NationalIdentity"/>
    <skos:definition xml:lang="en">Indicates a subject's identification with or labelling as Jewish or Israeli as a national identity.</skos:definition>
    <skos:definition xml:lang="fr">Indique qu'un sujet s'identifie à ou s'est vu assigner une identité nationale juive ou israélienne.</skos:definition>
    <skos:broader rdf:resource="#jewishlabel"/>
    <skos:closeMatch rdf:resource="http://dbpedia.org/resource/Israelis"/>
    <skos:relatedMatch rdf:resource="http://dbpedia.org/resource/Zionism"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </cwrc:NationalIdentity>
