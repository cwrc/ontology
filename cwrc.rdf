    <cwrc:represents rdf:resource="#blackEthnicity"/>
    <cwrc:represents rdf:resource="#blackRaceColour"/>
    <rdf:type rdf:resource="#TextLabels"/>
    <skos:inScheme rdf:resource="#TextLabels"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
