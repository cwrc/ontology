  <owl:Class rdf:ID="Award">
    <rdfs:label xml:lang="en">award</rdfs:label>
    <rdfs:label xml:lang="fr"></rdfs:label>
    <skos:definition xml:lang="en">An award is something given to a person, a group of people, like a sports team, or an organization in recognition of their excellence in a certain field. An award may be accompanied by a trophy, title, certificate, commemorative plaque, medal, badge, pin, or ribbon. The accompanying item may vary according to purpose. </skos:definition>
    <skos:definition xml:lang="fr"/>
    <skos:closeMatch rdf:resource="https://en.wikipedia.org/wiki/Award_or_decoration"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="EducationalAward">
    <rdfs:label xml:lang="en">educational award</rdfs:label>
    <rdfs:label xml:lang="fr">récompense scolaire</rdfs:label>
