    <owl:sameAs rdf:resource="http://vocab.getty.edu/aat/300266386"/>
    <skos:exactMatch rdf:resource="http://id.loc.gov/authorities/subjects/sh85089622"/>
    <skos:definition xml:lang="en">A person's name, containing a proper noun or proper-noun phrase referring to a person. Has subclasses for types of names such as @@#Pseudonym@@ and may have @@#NameType@@, and may have associated parts. This term is derived in part from the Text Encoding Initiative P5 schema <![CDATA[<i>persname</i>]]> element. See also @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-persName.html@@.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:ObjectProperty rdf:ID="hasName">
    <rdfs:label xml:lang="en">has name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">Indicates a person's name.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasNamePart">
    <rdfs:label xml:lang="en">has name part</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">Indicates a component of a name. Order of name parts is indicated by @@#hasSortOrder@@</skos:definition>
    <skos:definition xml:lang="fr"/>
    <cwrc:rangeIncludes rdf:resource="#PersonalName"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasSortOrder">
    <rdfs:label xml:lang="en">has sort order</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">Indicates the order in which the parts of a name occur.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <cwrc:rangeIncludes rdf:resource="#PersonalName"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="NameType">
    <rdfs:label xml:lang="en">name type</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#PersonalName"/>
    <skos:definition xml:lang="en">An aspect of a person's name, including its use, connotations, or significance. </skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="AuthorialName">
    <rdfs:label xml:lang="en">authorial name</rdfs:label>
    <rdfs:label xml:lang="fr">nom d'auteur·e</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A name under which a writer wrote or published.</skos:definition>
    <skos:definition xml:lang="fr">Un nom sous lequel un·e auteur·e a écrit ou publié.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="PreferredName">
    <rdfs:label xml:lang="en">preferred name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <cwrc:rangeIncludes rdf:resource="#PersonalName"/>
    <cwrc:rangeIncludes rdf:resource="#Organization"/>
    <skos:definition xml:lang="en">Indicates the form of name preferred by an authority or institution.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:ObjectProperty rdf:ID="preferredBy">
    <rdfs:label xml:lang="en">preferred by</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">Indicates the authority or institution by which the name form is preferred.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="Nickname">
    <rdfs:label xml:lang="en">nickname</rdfs:label>
    <rdfs:label xml:lang="fr">nom honnorifique</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">Indicates familiar, humorous, or abusive name given to a person, applied instead of or in addition to their real name. May have @@#NameType@@, @@#AbusiveName@@ or @@#HonorificName@@.</skos:definition>
    <skos:definition xml:lang="fr">Un nom à connotation positive qui glorifie ou honnore une personne.</skos:definition> 
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="AbusiveName">
    <rdfs:label xml:lang="en">abusive name</rdfs:label>
    <rdfs:label xml:lang="fr">nom abusif</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A name constructed with the intent to abuse, satirize, or slander. For example, Constance Gore-Booth was known in the press by the nickname of "Red Countess" and this nickname had negative connotations.</skos:definition>
    <skos:definition xml:lang="fr">Un nom inventé dans l'intention d'abuser, de tourner en satire ou de calomnier une personne. Par exemple, Constance Gore-Booth était connue dans la presse sous le surnom de "Red Countess" qui avait une connotation négative.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="HonorificName">
    <rdfs:label xml:lang="en">honorific name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A positive name which celebrates or honours a person. </skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="CrypticName">
    <rdfs:label xml:lang="en">cryptic name</rdfs:label>
    <rdfs:label xml:lang="fr">nom cryptique</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A name deliberately obscured, as in an anagram. Cryptic names have lexical meaning such as "The Scotch Milkmaid,""A Housewife," or "A Placid Reader." </skos:definition>
    <skos:definition xml:lang="fr">Un nom qu'on a délibérément chercher à cacher, par un exemple dans un anagramme. Les noms cryptiques ont une signification lexicale, par exemple "The Scotch Milkmaid", "A Housewife" ou "A Placid Reader".</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="LocalName">
    <rdfs:label xml:lang="en">local name</rdfs:label>
    <rdfs:label xml:lang="fr">nom local</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A name with a geographic connotation, for example, "Julian of Norwich."</skos:definition>
    <skos:definition xml:lang="fr">Nom avec une connotation géographique, par exmple "Julienne de Norwich."</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="RomanceName">
    <rdfs:label xml:lang="en">romance name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">Indicates cases where the person has a name associated with writing romances, or with romantic writing, for example, Swift's Stella.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="LiteraryName">
    <rdfs:label xml:lang="en">literary name</rdfs:label>
    <rdfs:label xml:lang="fr">nom littéraire</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A name applied to a writer by others, for example in reviews or at readings, which orginate in the person's status as a writer.</skos:definition>
    <skos:definition xml:lang="fr">Un nom attribué par d'autres à un·e auteur·e, par example dans des critiques ou lors de lectures, prenant son origine dans le statut d'écrivain·e de la personne.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="FamiliarName">
    <rdfs:label xml:lang="en">familiar name</rdfs:label>
    <rdfs:label xml:lang="fr">nom familier</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A version of a name used by those familiar with a person, such as family, friends, or co-workers.</skos:definition>
    <skos:definition xml:lang="fr">Variante du nom utilisée par les individus familiers avec une personne, telles que la famille, les ami·e·s ou les collègues.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="Pseudonym">
    <rdfs:label xml:lang="en">pseudonym</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">Indicates both names and descriptive phrases (e.g. "A Lover of Her Sex" is a pseudonym for Mary Astell) used in publishing to conceal the identity of the author or to represent a persona.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="Birthname">
    <rdfs:label xml:lang="en">birthname</rdfs:label>
    <rdfs:label xml:lang="fr">nom de naissance</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <owl:sameAs rdf:resource="http://vocab.getty.edu/aat/300404681"/>
    <skos:definition xml:lang="en">Indicates the name a person was assigned at birth.</skos:definition>
    <skos:definition xml:lang="fr">Indique le nom assigné à la naissance.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="Forename">
    <rdfs:label xml:lang="en">forename</rdfs:label>
    <rdfs:label xml:lang="fr">prénom</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <owl:sameAs rdf:resource="http://vocab.getty.edu/aat/300404681"/>
    <skos:definition xml:lang="en">A subject's given or baptismal name. This term is derived from the Text Encoding Initiative P5 schema <![CDATA[<i>forename</i>]]> element. See also @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-forename.html@@.</skos:definition>
    <skos:definition xml:lang="fr">Le nom attribué ou le nom de baptême d'une personne. Ce terme est dérivé de l'élément 'forename' du schéma P5 du Text Encoding Initiative. Voir l'élément TEI <![CDATA[<i>forename</i>]]>.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="Surname">
    <rdfs:label xml:lang="en">surname</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <owl:equivalentClass rdf:resource="http://metadataregistry.org/concept/show/id/8581.html"/>
    <skos:definition xml:lang="en">A subject's last name at birth, often a family (inherited) name, as opposed to a @@#Forename@@ or @@#Nickname@@." This term is derived from the Text Encoding Initiative P5 schema <![CDATA[<i>surname</i>]]> element. See also @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-surname.html@@.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="ProfessionalTitle">
    <rdfs:label xml:lang="en">professional title</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#RoleName"/>
    <skos:definition xml:lang="en">A person's professional role (e.g. Doctor Jane Smith) or designates membership in a professional society (e.g. John Smith, Esquire).</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="IndexedName">
    <rdfs:label xml:lang="en">indexed name</rdfs:label>
    <rdfs:label xml:lang="fr">nom indexé</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">Name under which a person's work is indexed by an institution or organization.</skos:definition>
    <skos:definition xml:lang="fr">Nom sous le travail d'une personne est indexé par une insitution ou une organisation. </skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:ObjectProperty rdf:ID="indexedBy">
    <rdfs:label xml:lang="en">indexed by</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">Indicates an organization's use of this name form in its index.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:ObjectProperty>

  <owl:Class rdf:ID="MarriedName">
    <rdfs:label xml:lang="en">married name</rdfs:label>
    <rdfs:label xml:lang="fr">nom marital</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A surname adopted in marriage. </skos:definition>
    <skos:definition xml:lang="fr">Nom de famille adopté après le mariage.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="ReligousName">
    <rdfs:label xml:lang="en">religious name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#NameType"/>
    <skos:definition xml:lang="en">A name acquired through a religious role or practice.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="RoyalName">
    <rdfs:label xml:lang="en">royal name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#RoleName"/>
    <skos:definition xml:lang="en">A name based on royal status.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="SelfConstructedName">
    <rdfs:label xml:lang="en">self constructed name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#AdditionalName"/>
    <skos:definition xml:lang="en">A self-chosen name used in everyday life rather than only on title-pages as is the case for a @@#pseudonym@@.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>
  <owl:Class rdf:ID="StyledName">
    <rdfs:label xml:lang="en">styled name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#RoleName"/>
    <skos:definition xml:lang="en">A courtesy title associated with the British or other peerage.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>
  <owl:Class rdf:ID="TitledName">
    <rdfs:label xml:lang="en">titled name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#RoleName"/>
    <skos:definition xml:lang="en">A formal title in the British and other peerages, including of a life peer. Peeresses may be described as titled, although in strict terms, unless one holds a title "in her own right", these are @@#StyledName@@s or courtesy titles.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="SystemName">
    <rdfs:label xml:lang="en">system name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <rdfs:subClassOf rdf:resource="#PersonalName"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/accountName"/>
    <skos:definition xml:lang="en">A name by which a person holds an account for a platform or service.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="RoleName">
    <rdfs:label xml:lang="en">role name</rdfs:label>
    <rdfs:label xml:lang="fr"/>
    <skos:definition xml:lang="en">A name which "indicates that a person has a particular role or position in society, such as an official title or rank." This term is derived from the Text Encoding Initiative P5 schema for the <![CDATA[<i>roleName</i>]]> element. See @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-roleName.html@@.</skos:definition>
    <skos:definition xml:lang="fr"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="AdditionalName">
    <rdfs:label xml:lang="en">additional name</rdfs:label>
    <rdfs:label xml:lang="fr">nom additionnel</rdfs:label>
    <skos:definition xml:lang="en">An additional name, "such as a nickname, epithet, or alias." This term is derived from the Text Encoding Initiative P5 schema for the <![CDATA[<i>addName</i>]]> element. See @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-addName.html@@.</skos:definition>
    <skos:definition xml:lang="fr">Un nom supplémentaire, "tel qu'un surnom, une épithète ou un alias". Ce terme est dérivé du schéma Text Encoding Initiative P5 pour l'élément <![CDATA[<i>addName</i>]]>. Voir l'élément TEI addName @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-addName.html@@.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="NameLink">
    <rdfs:label xml:lang="en">name link</rdfs:label>
    <rdfs:label xml:lang="fr">particule de nom</rdfs:label>
    <skos:definition xml:lang="en">"A connecting phrase or link used within a name but not regarded as part of it, such as van der or of." This term is derived from the Text Encoding Initiative P5 schema for the <![CDATA[<i>NameLink</i>]]> element. See @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-nameLink.html@@.</skos:definition>
    <skos:definition xml:lang="fr">"Une locution ou un lien de connexion utilisé dans un nom mais n'étant pas considéré comme partie intégrante de ce nom, par example van der ou de." Ce terme est dérivé du schéma P5 du Text Encoding Initiative pour l'élément <![CDATA[<i>NameLink</i>]]>. Voir l'élément TEI @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-nameLink.html@@.</skos:definition> 
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="GenerationalName">
    <rdfs:label xml:lang="en">generational name</rdfs:label>
    <rdfs:label xml:lang="fr">nom générationnel</rdfs:label>
    <skos:definition xml:lang="en">A name component used to distinguish otherwise similar names on the basis of the relative ages or generations of the persons named." This term is derived from the Text Encoding Initiative P5 schema for the <![CDATA[<i>genName</i>]]> element. See @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-genName.html@@.</skos:definition>
    <skos:definition xml:lang="fr">Composant de nom utilisé pour distinguer des noms similaires, en fonction de l'âge ou de la génération relatifs aux personnes portant un nom. "Ce terme est dérivé du schéma P5 du Text Encoding Initiative pour l'élément <![CDATA[<i>genName</i>]]>. Voir l'élément TEI genName @@http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-genName.html@@.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>



  <owl:Class rdf:ID="OccupationContext">
    <rdfs:label xml:lang="en">occupation context</rdfs:label>
    <rdfs:label xml:lang="fr">contexte biographique</rdfs:label>
    <rdfs:subClassOf rdf:resource="#BiographyContext"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">A significant subclass of @@#Context@@. Annotations typed as occupation provide information about and discussions of positions and activities significant to a person's life, including paid, unpaid, and voluntary work both beyond and within the home. Although writing as a career sometimes overlaps with occupation, literary activities are often described in writing contexts.</skos:definition>
    <skos:definition xml:lang="fr">Contexte Biographique est une sous-classe majeure de contexte. Les annotations entrées comme contexte biographique fournissent des informations et des discussions sur la biographie ou la vie personnelle d'un individu, grâce aux propriétés, aux relations biographiques et  aux sous-classes de contexte biographique, telles que le contexte de naissance, le contexte de forme culturelle ou le contexte professionnel.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

