    <rdfs:label xml:lang="fr">exposition (bourse)</rdfs:label>
    <skos:definition xml:lang="en">An exhibition is a type of scholarship award or bursary.</skos:definition>
    <skos:definition xml:lang="fr">Une exposition est un type de bourse ou de récompense académique.</skos:definition>
    <prov:wasDerivedFrom rdf:resource="https://en.wikipedia.org/wiki/Exhibition_(scholarship)"/>
    <rdfs:subClassOf rdf:resource="#EducationalAward"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
    <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

  <owl:Class rdf:ID="Distinction">
    <rdfs:label xml:lang="en">distinction</rdfs:label>
    <rdfs:label xml:lang="fr">mention</rdfs:label>
    <skos:definition xml:lang="en">An educational award indicating that a person has achieved honours or distinction in their studies.</skos:definition>
    <skos:definition xml:lang="fr">Un prix éducatif indiquant qu'une personne a obtenu des honneurs ou une mention au cours de ses études.</skos:definition>
    <rdfs:subClassOf rdf:resource="#EducationalAward"/>
    <rdfs:isDefinedBy rdf:resource="http://sparql.cwrc.ca/ontologies/cwrc-ONTOLOGY_DATE"/>
