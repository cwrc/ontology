  </owl:DeprecatedClass>
  <owl:DeprecatedClass rdf:ID="EthnicityContext">
    <rdfs:label xml:lang="en">racial, colour or ethnicity context</rdfs:label>
    <rdfs:label xml:lang="fr">contexte racial, de couleur de peau ou d'ethnicité</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
