  <cwrc:PoliticalAffiliation rdf:about="#irishRepublicanism">
    <skos:inScheme rdf:resource="#PoliticalAffiliation"/>
    <rdf:value>irish republicanism</rdf:value>
    <rdf:value>irish republican</rdf:value>
    <rdf:value>irish independence</rdf:value>
