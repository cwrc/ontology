  <owl:Class rdf:ID="Address">
    <rdfs:label xml:lang="en">address</rdfs:label>
    <rdfs:label xml:lang="fr">adresse</rdfs:label>
    <skos:definition xml:lang="en">A mailing or street address.</skos:definition>
    <skos:definition xml:lang="fr">Une adresse postale ou de rue.</skos:definition>
    <rdfs:comment xml:lang="en">CWRC address is the equivalent of a schema.org Postal Address and uses the predicates from schema Postal Address.</rdfs:comment>
    <owl:sameAs rdf:resource="http://schema.org/PostalAddress"/>
    <rdfs:subClassOf rdf:resource="#Place"/>
